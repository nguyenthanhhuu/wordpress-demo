{
    "name": "oxygenna/oxygenna-bbpress",
    "type": "wordpress-theme",
    "description": "Oxygenna BBPress Boostrap",
    "license": "ThemeForest",
    "authors": [
        {
            "name": "John Langan",
            "email": "john.langan@oxygenna.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "oxygenna/oxygenna-php-standards": "1.0.*",
